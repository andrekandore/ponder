FalseString: WOString {
	escapeHTML = false;
	value = d2wContext.falseString;
}

IsFalse: WOConditional {
	condition = isFalse;
}

IsTrue: WOConditional {
	condition = objectPropertyValue;
}

TrueString: WOString {
	escapeHTML = false;
	value = d2wContext.trueString;
}

IsNull: WOConditional {
	condition = objectPropertyValueIsNonNull;
	negate = true;
}

NullString: WOString {
	escapeHTML = false;
	value = d2wContext.nullString;
}

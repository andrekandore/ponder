EditButton: R2DLinkButton {
	action = editObjectAction;
	class = "edit";
	disabled = isNotEditable;
	title = d2wContext.displayNameForEditConfiguration;
	value = d2wContext.editButtonLabel;
}

EditButtonContent: WOSwitchComponent {
	WOComponentName = d2wContext.buttonContentComponentName;
	localContext = d2wContext;
	buttonLabel = d2wContext.editButtonLabel;
}

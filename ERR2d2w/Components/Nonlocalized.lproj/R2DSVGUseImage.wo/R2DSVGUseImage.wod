SVGElement: WOGenericContainer {
	elementName = "svg:svg";
	"xmlns:svg" = "http://www.w3.org/2000/svg";
	"xmlns:xlink" = "http://www.w3.org/1999/xlink";
	version = "1.1";
	viewBox = ^viewBox;
	height = ^imageHeight;
	width = ^imageWidth;
	class = ^imageClass;
}

SVGUseElement: WOGenericElement {
	elementName = "svg:use";
	"xlink:href" = imageHref;
}
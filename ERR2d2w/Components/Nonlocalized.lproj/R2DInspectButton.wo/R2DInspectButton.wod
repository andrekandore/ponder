InspectButton: WOSubmitButton {
	action = inspectObjectAction;
	value = d2wContext.inspectButtonLabel;
}

InspectLink: WOHyperlink {
	action = inspectObjectAction;
	string = d2wContext.inspectButtonLabel;
}

IsButton: WOConditional {
	condition = context.isInForm;
}

IsLink: WOConditional {
	condition = context.isInForm;
	negate = true;
}
